@{
    Layout = null;
    var BaseUrl = ViewBag.BaseUrl;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Quản Trị Nông Thôn Mới Huyện Quỳ Hợp</title>
    <link rel="stylesheet" href="./css/login.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
</head>
<body>
    <div class="container">
        <div class="login-box">
            <img src="https://nongthonmoi.vnptnghean.com.vn/images/logo/quyhop.png" alt="Quy Hop Logo" class="logo">
            <div class="login-form">
                <h2>Đăng Nhập Tài Khoản</h2>
                <label for="username">Tài khoản</label>
                <input type="text" id="username" name="username" placeholder="Nhập tên tài khoản" required>
                <label for="password">Mật khẩu</label>
                <input type="password" id="password" name="password" placeholder="Nhập mật khẩu" required>
                <button id="login-button" type="submit">Đăng nhập</button>
            </div>
        </div>
    </div>
</body>
</html>
<script src="/js/login.js"></script>
